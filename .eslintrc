{
  "root": true,
  "env": {
    "node": true
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/typescript",

    "plugin:prettier/recommended",
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    /*eslint max-len: ["error", { "code": 80 }]*/
  }
}
